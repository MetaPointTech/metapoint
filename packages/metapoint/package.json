{
  "name": "metapoint",
  "version": "0.1.5",
  "description": "âš¡Meta first and low-code. Peer-to-Peer typesafe APIs or Channels made easy.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cts"
  },
  "keywords": [
    "libp2p",
    "channel",
    "zod",
    "low-code",
    "p2p",
    "subscription"
  ],
  "author": "sovlookup <gonorth@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/SOVLOOKUP/metapoint",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SOVLOOKUP/metapoint"
  },
  "bugs": {
    "url": "https://github.com/SOVLOOKUP/metapoint/issues"
  },
  "files": [
    "dist/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsup --cjsInterop && vite build",
    "test": "vitest run"
  },
  "dependencies": {
    "@chainsafe/libp2p-noise": "^14.0.0",
    "@libp2p/mplex": "^10.0.5",
    "@libp2p/websockets": "^8.0.5",
    "@multiformats/multiaddr": "^12.1.11",
    "libp2p": "^1.0.8",
    "libp2p-transport": "workspace:*",
    "nanoid": "^5.0.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "vite": "^5.0.5"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "clean": true,
    "dts": true,
    "treeshake": true,
    "shims": true
  }
}