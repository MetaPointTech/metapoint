{
  "name": "libp2p-transport",
  "version": "0.4.1",
  "description": "peer to peer rpc/channel communication based on libp2p",
  "keywords": [
    "libp2p",
    "channel",
    "rpc"
  ],
  "author": "sovlookup <gonorth@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/SOVLOOKUP/metapoint/tree/master/packages/core",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SOVLOOKUP/metapoint"
  },
  "bugs": {
    "url": "https://github.com/SOVLOOKUP/metapoint/issues"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsup --cjsInterop",
    "test": "vitest run",
    "bench": "vitest bench"
  },
  "dependencies": {
    "@libp2p/interface": "^0.1.5",
    "@libp2p/peer-id": "^3.0.5",
    "@multiformats/multiaddr": "^12.1.9",
    "@xobj/core": "^1.3.1",
    "libp2p": "^0.46.18",
    "pino": "^8.16.1",
    "queueable": "^5.3.2",
    "streaming-iterables": "^8.0.1"
  },
  "devDependencies": {
    "@chainsafe/libp2p-noise": "^13.0.2",
    "@libp2p/mplex": "^9.0.10",
    "@libp2p/websockets": "^7.0.11",
    "binconv": "^0.2.0",
    "destr": "^2.0.2",
    "fastify": "^4.24.3"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "clean": true,
    "dts": true,
    "treeshake": true,
    "shims": true
  }
}